<script src="http://js.pusher.com/2.0/pusher.min.js" type="text/javascript"></script>

<script>
    $(document).ready(function () {
        $("#chat_div").chatbox({ id: "chat_div",
            title: "Chat",
            user: "test",
            offset: 20,
            messageSent: function (id, user, msg) {
                FB.api('/me', function (response) {
                    $.ajax({
                        url: '/Chat/SendMessage/',
                        data: { message: msg, username: response.first_name},
                        type: 'POST'
                    });
                });
            }
        });

        var pusher = new Pusher('fe769be86f1e807ab53c');
        var channel = pusher.subscribe('test_channel');
        channel.bind('test_event', function (data) {
            $("#chat_div").chatbox("option", "boxManager").addMsg(data.username, data.message);
            $.ajax({
                url: '/Chat/AddChatHistory/',
                data: { chatMsgId: data.chatMsgId },
                type: 'POST'
            });
        });
    });
</script>
<div id="chat_div"></div>

